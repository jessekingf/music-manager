{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/MusicManager/bin/Debug/net9.0/MusicManager",
      "args": [
        "${env:USERPROFILE}/Music/"
      ],
      "cwd": "${workspaceFolder}/src",
      "stopAtEntry": false
    }
  ]
}
